    "file": "LICENSE"
  },
  "platforms": {
    "ios": "10.0"
  },
  "swift_versions": "5.0",
  "source": {
    "git": "https://github.com/appwise-labs/PaginationTracker.git",
    "tag": "1.0.2"
  },
  "default_subspecs": [
    "Core",
    "CoreData"
  ],
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Sources/Core/**/*.swift",
      "dependencies": {
        "Alamofire": [
          "~> 4.8"
        ],
        "StatefulUI": [

        ]
      }
    },
    {
      "name": "CoreData",
      "source_files": "Sources/CoreData/**/*.swift",
      "dependencies": {
        "PaginationTracker/Core": [

        ],
        "AppwiseCore/CoreData": [

        ]
      }
    }
  ],
  "swift_version": "5.0"
}
